<resources>


    <style name="AppTheme" parent="Base.Theme.AppCompat.Light">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkPre23</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:fontFamily">@font/montserrat</item>
        <item name="android:windowSharedElementEnterTransition">@transition/hero_transition</item>
        <item name="android:windowSharedElementExitTransition">@transition/hero_transition</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
    </style>

</resources>
